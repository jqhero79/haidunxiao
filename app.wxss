/**app.wxss**/
page{
  background-color:#ffffff;
  font-size: 12px;
  font-family: "Microsoft YaHei", "Helvetica Neue", "Hiragino Sans GB", "Segoe UI", Tahoma, Arial, STHeiti, sans-serif;
  _font-family: Tahoma, Arial, Helvetica, STHeiti, sans-serif;
  color: #333333;
  font-weight: normal;
  font-style: normal;
  height: 100%;
  width: 100%;
}

.no_list{width: 100%;
  text-align: center;
  display: inline-block;
  justify-content: center;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #8C8C8C;
}

.popupField_box {
  padding-bottom: env(safe-area-inset-bottom);
}

.edit{padding: 0 32rpx;position: relative;}
.commonBottom{width: 100%;height: 400rpx;}
.commonBottomBtn{
  position: fixed;
  bottom: 24rpx;left: 32rpx;
  width: 686rpx;
  height: 88rpx;
  background-color: #000000;
  line-height: 88rpx;
  text-align: center;
  color: #D9BF8D;
  font-size: 28rpx;
  border-radius: 44rpx;
  z-index: 100;
  font-weight: 600;
  margin-bottom: env(safe-area-inset-bottom);
}

/* 底部悬浮按钮 */
.addBoottomSuspensionBtn{
  height: 88rpx;
  margin:24rpx auto;
  display: flex;
  justify-content: center;
  position: fixed;
  bottom: 0;left: 0;right: 0;
  z-index: 99;
}
.addBoottomSuspensionBtn .box{
  height: 100%;
  border-radius:44rpx;
  box-shadow:0px 2px 8px 0px rgba(0,0,0,0.2);
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  padding: 0 40rpx;
}
.addBoottomSuspensionBtn .addBtns{width: 48rpx;height: 48rpx;margin-right: 16rpx;}
.addBoottomSuspensionBtn .Name{font-weight:600;color: #000000;font-size: 28rpx;}

.simple_Btn_Sure{width: 686rpx;height: 88rpx;background-color: #F5F7FA;color: #000000;text-align: center;line-height: 88rpx;border-radius: 44rpx;margin:24rpx 0;font-weight:600;display: inline-block;}